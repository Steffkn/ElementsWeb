@model IEnumerable<Elements.Models.User>
@{
    ViewData["Title"] = "Manage users";
}

<h2>ManageUsers</h2>

<div class="row">
    <div class="col">
        <table class="table table-dark table-striped">
            <thead>
                <tr>
                    <th>UserName</th>
                    <th>NormalizedEmail</th>
                    <th>EmailConfirmed</th>
                    <th>RegisterDate</th>
                </tr>
            </thead>
            <tfoot>
                <tr>
                    <td></td>
                    <td></td>
                    <td></td>
                    <td></td>
                </tr>
            </tfoot>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@(item.UserName)</td>
                        <td>@(item.NormalizedEmail)</td>
                        <td>@(item.EmailConfirmed)</td>
                        <td>@(item.RegisterDate)</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>