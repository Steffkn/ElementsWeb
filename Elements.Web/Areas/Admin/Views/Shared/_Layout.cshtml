<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin panel - Elements.Web</title>

    <environment include="Development">
        <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.css" />
        <link rel="stylesheet" href="~/css/site.css" />
    </environment>
    <environment exclude="Development">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.2/css/bootstrap.min.css"
              integrity="sha384-Smlep5jCw/wG7hdkwQ/Z5nLIefveQRIY9nfy6xoR1uRYBtpZgI6339F5dgvm/e9B" crossorigin="anonymous"
              asp-fallback-href="~/lib/bootstrap/dist/css/bootstrap.min.css"
              asp-fallback-test-class="sr-only" asp-fallback-test-property="position" asp-fallback-test-value="absolute" />
        <link rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />
    </environment>
    <link rel="stylesheet" href="~/lib/fontawesome/css/all.css" />
    <link href="~/css/dashboard.css" rel="stylesheet" />
</head>
<body>
    <div class="row no-gutters h-100">
        <nav id="sidebar" class="bg-secondary">
            <div class="sidebar-header">
                <h3>Admin panel</h3>
                <strong>
                    <i class="fas fa-align-justify"></i>
                </strong>
            </div>

            <ul class="nav flex-column">
                <li class="nav-item">
                    <a asp-action="ManageUsers" class="nav-link">
                        <i class="fas fa-user"></i>
                        Users
                    </a>
                </li>
                <li class="nav-item">
                    <a asp-action="ManageCategories" class="nav-link">
                        <i class="fas fa-wrench"></i>
                        Categories
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#homeSubmenu" data-toggle="collapse" aria-expanded="false" class="dropdown-toggle">
                        <i class="fas fa-home"></i>
                        Home
                    </a>
                    <ul class="collapse list-unstyled" id="homeSubmenu">
                        <li class="nav-item">
                            <a href="#">Forum</a>
                        </li>
                        <li class="nav-item">
                            <a href="#">Forum</a>
                        </li>
                        <li class="nav-item">
                            <a href="#">News</a>
                        </li>
                    </ul>
                </li>
            </ul>
        </nav>

        <div>
            <button type="button" id="sidebarCollapse" class="btn btn-info">
                <i class="fas fa-align-left"></i>
                asdasd
            </button>
            <button class="btn btn-dark d-inline-block d-lg-none ml-auto" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <i class="fas fa-align-justify"></i>
                asd
            </button>
            <div class="collapse navbar-expand" id="navbarSupportedContent">
                <ul class="nav navbar-nav ml-auto">
                    <li class="nav-item">
                        <a asp-area="" asp-controller="Home" asp-action="Index" class="nav-link">Back to site</a>
                    </li>
                </ul>
            </div>
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
    </div>



